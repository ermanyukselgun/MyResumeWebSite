@model MyResumeWebSite.Models.ProgramKnowledgeModelView


@{
    ViewBag.Title = "AddWebTech";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Add New Web Technology</h2>

<hr />
<br />

@using (Html.BeginForm("AddWebTech", "ProgramKnowledge", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(i => i.Web_Tech.Technology)
        @Html.TextBoxFor(i => i.Web_Tech.Technology, new { @class = "form-control", @required = "required" })
    </div>
    <div class="form-group">
        @Html.LabelFor(i => i.Web_Tech.Knowledge_level)
        @Html.TextBoxFor(i => i.Web_Tech.Knowledge_level, new { @class = "form-control", @required = "required" })
    </div>

    @Html.HiddenFor(i => i.Web_Tech.Id)
    @Html.ActionLink("Previous Page", "Index", null, new { @class = "btn btn-warning" })
    <button type="submit" class="btn btn-primary">Save</button>
}


@model MyResumeWebSite.Models.ProgramKnowledgeModelView
@{
    ViewBag.Title = "AddLanguage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Add New Language</h2>

<hr />
<br />

@using (Html.BeginForm("AddLanguage", "ProgramKnowledge", FormMethod.Post))
{    
    <div class="form-group">
        @Html.LabelFor(i => i.Prog_Language.Language)
        @Html.TextBoxFor(i => i.Prog_Language.Language, new { @class = "form-control", @required = "required" })
    </div>
    

    @Html.HiddenFor(i => i.Prog_Language.Id)
    @Html.ActionLink("Previous Page", "Index", null, new { @class = "btn btn-warning" })
    <button type="submit" class="btn btn-primary">Save</button>

}
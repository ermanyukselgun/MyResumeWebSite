@model MyResumeWebSite.Models.ProgramKnowledgeModelView

@{
    ViewBag.Title = "AddGameTech";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2>Add New Game Technology</h2>

<hr />
<br />

@using (Html.BeginForm("AddGameTech", "ProgramKnowledge", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(i => i.Game_Tech.GameEngine)
        @Html.TextBoxFor(i => i.Game_Tech.GameEngine, new { @class = "form-control", @required = "required" })
    </div>
    <div class="form-group">
        @Html.LabelFor(i => i.Game_Tech.Knowledge_level)
        @Html.TextBoxFor(i => i.Game_Tech.Knowledge_level, new { @class = "form-control", @required = "required" })
    </div>
    @Html.HiddenFor(i => i.Game_Tech.Id)
    @Html.ActionLink("Previous Page", "Index", null, new { @class = "btn btn-warning" })
    <button type="submit" class="btn btn-primary">Save</button>
}